type: txt
help: Local IPv6 modify ruleset name for interface
allowed: local -a params
	eval "params=($(cli-shell-api listActiveNodes firewall ipv6-modify))"
	echo -n "${params[@]}"

update:	ifname=$VAR(../../../@)
        /usr/sbin/ubnt-fw update-interface \
		update $ifname local $VAR(@) ipv6-modify


delete:	ifname=$VAR(../../../@)
	/usr/sbin/ubnt-fw update-interface \
		delete $ifname local $VAR(@) ipv6-modify
